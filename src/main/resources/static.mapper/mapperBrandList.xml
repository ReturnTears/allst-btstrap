<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="allst.boot.strap.dao.impl.BrandListsDaoImpl" xmlns="http://mybatis.org/schema/mybatis-mapper">
    <resultMap id="brandListMap" type="java.util.HashMap">
        <id column="id" property="id"/>
        <result column="brandID" property="brandID"/>
        <result column="brandName" property="brandName"/>
        <result column="brandCTime" property="brandCTime"/>
    </resultMap>
    <sql id="Base_Column_List">
        brand.id,
        brand.brandID,
        brand.brandName,
        brand.brandCTime
    </sql>
    <select id="brandListInfo" resultMap="brandListMap">
        select
        <include refid="Base_Column_List"/>
        from brand
    </select>
</mapper>